<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
        
<mapper namespace="com.dcits.dao.IStudentDAO">
	<resultMap id="StudentMap" type="com.dcits.entity.Student">
          <result property="id" column="id"/>
         <result property="name" column="name" />
     </resultMap>
     
     
     
    <!--查询所有-->
    <select id="getAll" resultMap="StudentMap">
        select  id,name from student
    </select>
    
    
    <!--新增学生-->
    <select id="addStudent"  resultType="com.dcits.entity.Student">
       insert  into student(name) value(#{name})
    </select>
    
         
    <!--查询所有-->
    <select id="getcursor" resultMap="StudentMap">
        select  id,name from student
    </select>
    
</mapper>